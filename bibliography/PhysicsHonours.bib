	number = {11},
	urldate = {2018-02-11},
	journal = {ACS Nano},
	author = {Li, Hai and Wu, Jumiati and Huang, Xiao and Lu, Gang and Yang, Jian and Lu, Xin and Xiong, Qihua and Zhang, Hua},
	month = nov,
	year = {2013},
	pages = {10344--10353},
