	file = {Full Text PDF:C\:\\Users\\mr_g.MR-G\\Zotero\\storage\\78347GA3\\Reed et al. - 2010 - The Effective Fine-Structure Constant of Freestand.pdf:application/pdf;Snapshot:C\:\\Users\\mr_g.MR-G\\Zotero\\storage\\5JYP9G8T\\tab-pdf.html:text/html}
}

