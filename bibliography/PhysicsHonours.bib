@article{ong_theory_2013,
	title = {Theory of remote phonon scattering in top-gated single-layer graphene},
	volume = {88},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.88.045405},
	doi = {10.1103/PhysRevB.88.045405},
	abstract = {We extend the theory of interfacial plasmon-phonon scattering to top-gated single-layer graphene. As with bottom-gated graphene, interfacial plasmon-phonon (IPP) modes are formed from the coupling between the graphene plasmon and the surface polar phonon modes in the top and bottom oxides. We study the effect of the top oxide thickness on dynamic screening and electron-IPP coupling. The remote phonon-limited electron mobility μRP and electron scattering rates in a HfO2-covered, SiO2-supported single-layer graphene are computed at various electron densities n for different dielectric thickness tox. We find that μRP is much more dependent on tox at low n. They also agree with the experimentally estimated room temperature μRP from Zou et al. [Phys. Rev. Lett. 105, 126601 (2010)]. The electron density dependent μRP is predicted to be between 5500 and 24200 cm2V−1s−1 from n=1012 to 1013 cm−2 at 300 K.},
	number = {4},
	urldate = {2018-02-27},
	journal = {Physical Review B},
	author = {Ong, Zhun-Yong and Fischetti, Massimo V.},
	month = jul,
	year = {2013},
	pages = {045405},
	file = {APS Snapshot:C\:\\Users\\mr_g.MR-G\\Zotero\\storage\\6ECHQCM6\\PhysRevB.88.html:text/html;Full Text PDF:C\:\\Users\\mr_g.MR-G\\Zotero\\storage\\VY4T46N4\\Ong and Fischetti - 2013 - Theory of remote phonon scattering in top-gated si.pdf:application/pdf}
}

@article{konar_effect_2010,
